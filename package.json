{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=development electron main.js",
    "lint": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "devDependencies": {
    "babel-eslint": "6.0.0-beta.5",
    "babel-preset-stage-0": "^6.5.0",
    "electron-debug": "^0.5.2",
    "electron-prebuilt": "^0.36.0",
    "eslint": "^2.2.0",
    "eslint-config-xo-react": "^0.5.0",
    "eslint-config-xo-space": "^0.11.0",
    "eslint-plugin-react": "^4.2.1",
    "react-hot-loader": "^1.3.0",
    "xo": "^0.12.0"
  },
  "dependencies": {
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.5.2",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "thunk": "0.0.1",
    "zmq": "2.14.0",
    "msgpack": "1.0.2"
  },
  "eslintConfig": {
    "extends": [
      "xo-space",
      "xo-react"
    ],
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "modules": true
    }
  },
  "xo": {
    "space": 2,
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ],
      "react/jsx-indent-props": [2, 2],
      "react/jsx-indent": [2, 2]
    },
    "globals": [
      "window",
      "document",
      "jest",
      "describe",
      "it",
      "expect",
      "beforeEach",
      "afterEach",
      "xit"
    ],
    "envs": {
      "node": true,
      "browser": true,
      "jquery": true,
      "es6": true,
      "jasmine": true
    },
    "esnext": true,
    "extends": [
      "xo-react"
    ],
    "ignores": [
      "vendor/**"
    ]
  }
}
